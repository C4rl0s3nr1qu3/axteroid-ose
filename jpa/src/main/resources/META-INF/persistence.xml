		<class>com.axteroid.ose.server.jpa.model.SunatPadronContribuyentePK</class>
		<class>com.axteroid.ose.server.jpa.model.SunatParametro</class>
		<class>com.axteroid.ose.server.jpa.model.SunatParametroPK</class>
		<class>com.axteroid.ose.server.jpa.model.SunatPlazosExcepcionales</class>

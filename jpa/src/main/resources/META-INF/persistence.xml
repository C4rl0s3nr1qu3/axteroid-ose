		<class>com.axteroid.ose.server.jpa.model.SunatAutorizacionComprobPagoFisicoPK</class>
		<class>com.axteroid.ose.server.jpa.model.SunatAutorizacionRangosContingencia</class>
		<class>com.axteroid.ose.server.jpa.model.SunatAutorizacionRangosContingenciaPK</class>
		<class>com.axteroid.ose.server.jpa.model.SunatCertificadoEmisor</class>
		<class>com.axteroid.ose.server.jpa.model.SunatCertificadoEmisorPK</class>
		<class>com.axteroid.ose.server.jpa.model.SunatComprobantesPagoElectronicos</class>
		<class>com.axteroid.ose.server.jpa.model.SunatComprobantesPagoElectronicosPK</class>
